{
  "manifest_version": 2,

  "name": "Base64 URL Decoder",
  "description": "Automatically decode base64 strings found in the URL",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
